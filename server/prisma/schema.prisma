generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model user {
  id             String   @id @default(auto()) @map("_id") @db.ObjectId
  nome           String
  email          String   @unique
  celular        String   @unique
  img_profile    String
  senha          String
  dt_atualizacao DateTime @db.Date
  dt_cadastro    DateTime @db.Date
}
